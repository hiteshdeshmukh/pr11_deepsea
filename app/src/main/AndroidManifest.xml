<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.POST_NOTIFICATIONS"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pr11_deepsea"
        tools:targetApi="31">
        <activity
            android:name=".ui.Features.DashboardSchedulerActivity"
            android:exported="false" />
        <activity
            android:name=".ui.Features.HomeTestResultActivity"
            android:exported="false" />
        <activity
            android:name=".ui.SignInPages.SplashScreenActivity"
            android:exported="true"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.Features.EditProfileActivity"
            android:exported="false"
            android:noHistory="true" />
        <activity
            android:name=".ui.Features.TestQuestionsPage"
            android:exported="false"
            android:noHistory="true" />
        <activity
            android:name=".ui.SignInPages.SignUpActivity"
            android:exported="false"
            android:noHistory="true" />
        <activity
            android:name=".ui.SignInPages.SignInActivity"
            android:exported="true"
            android:noHistory="true" />
        <activity
            android:name=".ui.Features.AddPostActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name" />

        <receiver android:name=".ui.Features.Receiver.AlarmReceiver"/>


    </application>








</manifest>